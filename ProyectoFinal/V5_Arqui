swr $0, #$0000 -- Resultado resta contador
swr $1, #$0007 -- (2n - 1) -- 7
swr $2, #$0000 -- 2i
swr $3, #$0001 -- 2i + 1
swr $4, #$0001 -- 2i + 1
swr $5, #$0003 -- minX
swr $6, #$0007 -- minY
swr $7, #$0003 -- maxX
swr $8, #$0007 -- maxY
sw $0, #$0003 -- X1
sw $1, #$0007 -- Y1
sw $2, #$0005 -- X2
sw $3, #$0004 -- Y2
sw $4, #$0005 -- X3
sw $5, #$0007 -- Y3
sw $6, #$0000 -- X4
sw $7, #$0005 -- Y4
checkVals:
add $4, #0, $3
str_if_neg_sub $5, 0($2), $5
str_if_neg_sub $7, $7, 0($2) 
add $2, #2, $2
str_if_neg_sub $6, 0($3), $6
str_if_neg_sub $8, $8, 0($3)                    
add $3, #2, $3
sub $0, $1, $4
BEQ FIN
BRA checkVals
FIN: BRA FIN

