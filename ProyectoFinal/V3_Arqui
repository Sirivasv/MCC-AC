swr $0, #$0000 -- Resultado resta contador
swr $1, #$0007 -- (2n - 1) -- 7
swr $2, #$0000 -- i
swr $3, #$0001 -- i + 1
swr $4, #$0003 -- minX
swr $5, #$0007 -- minY
swr $6, #$0003 -- maxX
swr $7, #$0007 -- maxY
sw $0, #$0003 -- X1
sw $1, #$0007 -- Y1
sw $2, #$0005 -- X2
sw $3, #$0004 -- Y2
sw $4, #$0005 -- X3
sw $5, #$0007 -- Y3
sw $6, #$0000 -- X4
sw $7, #$0005 -- Y4
checkVals: str_if_neg_sub $4, 0($2), $4
str_if_neg_sub $6, $6, 0($2) 
add $2, #1, $2
str_if_neg_sub $5, 0($3), $5
str_if_neg_sub $7, $7, 0($3)                    
add $3, #1, $3
sub $0, $1, $3
BEQ FIN
BRA checkVals
FIN: BRA FIN

